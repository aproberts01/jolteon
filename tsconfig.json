{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@lib/*": ["lib/*"],
      "@components/*": ["app/components/*"],
      "@prisma/*": ["prisma/*"],
      "@generated/prisma": ["generated/prisma/*"],
      "@api/*": ["app/api/*"],
      "@auth/*": ["app/api/auth/*"],
      "@providers": ["app/providers"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "app/api/auth/[...nextauth]"  ],
  "exclude": [
    "node_modules"
  ]
}
